<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema version="1.0" targetNamespace="http://LavadoFacilWebServices/" xmlns:tns="http://LavadoFacilWebServices/" xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:element name="BuscarPersona" nillable="true" type="tns:buscarPersona"/>

  <xs:element name="BuscarPersonaResponse" nillable="true" type="tns:buscarPersonaResponse"/>

  <xs:element name="LoginCliente" nillable="true" type="tns:loginCliente"/>

  <xs:element name="LoginClienteResponse" nillable="true" type="tns:loginClienteResponse"/>

  <xs:element name="WebServiceException" type="tns:WebServiceException"/>

  <xs:complexType name="buscarPersona">
    <xs:sequence>
      <xs:element name="ced" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="buscarPersonaResponse">
    <xs:sequence>
      <xs:element name="return" type="tns:persona" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="persona" abstract="true">
    <xs:sequence>
      <xs:element name="cedula" type="xs:string" minOccurs="0"/>
      <xs:element name="celular" type="xs:string" minOccurs="0"/>
      <xs:element name="fechaOlvidoPass" type="xs:dateTime" minOccurs="0"/>
      <xs:element name="nombre" type="xs:string" minOccurs="0"/>
      <xs:element name="passw" type="xs:string" minOccurs="0"/>
      <xs:element name="telefono" type="xs:string" minOccurs="0"/>
      <xs:element name="ubicacion" type="tns:ubicacion" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="ubicacion">
    <xs:sequence>
      <xs:element name="barrio" type="xs:string" minOccurs="0"/>
      <xs:element name="ciudad" type="xs:string" minOccurs="0"/>
      <xs:element name="direccion" type="xs:string" minOccurs="0"/>
      <xs:element name="id" type="xs:int"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="WebServiceException">
    <xs:sequence>
      <xs:element name="faultInfo" type="xs:string" minOccurs="0"/>
      <xs:element name="message" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="loginCliente">
    <xs:sequence>
      <xs:element name="ced" type="xs:string" minOccurs="0"/>
      <xs:element name="passw" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="loginClienteResponse">
    <xs:sequence>
      <xs:element name="return" type="tns:persona" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
</xs:schema>

